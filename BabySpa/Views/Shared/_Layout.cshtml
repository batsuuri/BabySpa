<!DOCTYPE html>
<html lang="en" class="full-height">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Baby Spa</title>
    <!-- Meta OG -->
    <meta property="og:title" content="Modern Spa Landing Page">
    <meta property="og:description" content="Engage your clients and provide them with an elegant spa landign page enhanced with Material Design.">
    <meta property="og:image" content="https://mdbootstrap.com/img/Live/MDB/spa-landing.jpg">
    <meta property="og:url" content="https://mdbootstrap.com/live/_MDB/templates/Landing-Page/spa-landing-page.html">
    <meta property="og:site_name" content="mdbootstrap.com">
    <!-- /Meta OG -->
    <!-- Twitter Card -->
    <meta name="twitter:card" content="summary_large_image">
    <meta name="twitter:description" content="Engage your clients and provide them with an elegant spa landign page enhanced with Material Design.">
    <meta name="twitter:title" content="Modern Spa Landing Page">
    <meta name="twitter:site" content="BabySpa">
    <meta name="twitter:image" content="https://mdbootstrap.com/img/Live/MDB/spa-landing.jpg">
    <meta name="twitter:creator" content="BabySpa">
    <!-- /Twitter Card -->

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <style type="text/css" id="#jarallax-clip-0">
        #jarallax-container-0 {
            clip: rect(0 1423px 720px 0);
            clip: rect(0, 1423px, 720px, 0);
        }
    </style>

    
</head>
@{ string type = "";
    string path = Request.Url.GetLeftPart(UriPartial.Authority);
}
<body class="light-blue-skin intro-page spa-lp">
    <!--Navigation & Intro-->
    <header>
        <!--Navbar-->
        <nav class="navbar fixed-top navbar-expand-lg navbar-dark scrolling-navbar top-nav-collapse" style="">
            <div class="container">
                <a class="navbar-brand waves-effect waves-light" href="@path#"><strong>Navbar</strong></a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation" style="">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarSupportedContent">
                    <!--Links-->
                    <ul class="navbar-nav mr-auto smooth-scroll">
                        <li class="nav-item">
                            <a class="nav-link waves-effect waves-light" href="@path#home">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link waves-effect waves-light" href="@path#about" data-offset="100">About</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link waves-effect waves-light" href="@path#services" data-offset="100">Services</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link waves-effect waves-light" href="@path#attractions" data-offset="100">Attractions</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link waves-effect waves-light" href="@path#testimonials" data-offset="100">Opinions</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link waves-effect waves-light" href="@path#newsletter" data-offset="100">Newsletter</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link waves-effect waves-light" data-toggle="modal" data-target="#modal-contact">Contact</a>
                        </li>
                    </ul>
                    <!--Social Icons-->
                    <ul class="navbar-nav nav-flex-icons">
                        <li class="nav-item">
                            <a class="nav-link waves-effect waves-light"><i class="fa fa-facebook"></i></a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link waves-effect waves-light"><i class="fa fa-twitter"></i></a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link waves-effect waves-light"><i class="fa fa-instagram"></i></a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
        <!--/Navbar-->
        <!-- Intro Section -->
        <div id="home" class="view hm-black-strong-1 jarallax" data-jarallax="{&quot;speed&quot;: 0.2}" style="background-image: none; z-index: 0; background-attachment: scroll; background-size: auto;" data-jarallax-original-styles="background-image: url(https://mdbootstrap.com/img/Photos/Horizontal/Relax/full%20page/spa.jpg);">
            <div class="full-bg-img">
                <div class="container flex-center">
                    <div class="row smooth-scroll">
                        <div class="col-md-12 white-text text-center">
                            <div class="wow fadeInDown" data-wow-delay="0.2s" style="visibility: visible; animation-delay: 0.2s; animation-name: fadeInDown;">
                                <h3 class="display-3 font-bold mb-2">Baby Spa</h3>
                                <hr class="hr-light">
                                <h4 class="subtext-header mt-4 mb-5">Lorem ipsum dolor sit amet, consectetur adipisicing elit. <p> Deleniti consequuntur, nihil voluptatem modi nobis veniam.</p></h4>
                            </div>
                            <a class="btn btn-rounded btn-lg btn-purple-2 wow fadeInUp waves-effect waves-light" data-wow-delay="0.2s" data-toggle="modal" data-target="#modal-reservation" style="visibility: visible; animation-delay: 0.2s; animation-name: fadeInUp;">Make a reservation</a>
                        </div>
                    </div>
                </div>
            </div>
            <div id="jarallax-container-0" style="position: absolute; top: 0px; left: 0px; width: 100%; height: 100%; overflow: hidden; pointer-events: none; visibility: hidden; z-index: -100;"><div style="background-position: 50% 50%; background-size: 100%; background-repeat: no-repeat; background-image: url(&quot;https://mdbootstrap.com/img/Photos/Horizontal/Relax/full%20page/spa.jpg&quot;); position: fixed; top: 0px; left: 0px; width: 1423px; height: 800.438px; overflow: hidden; pointer-events: none; margin-left: 0px; margin-top: -40.2188px; visibility: visible; transform: translateY(-28.4px) translateZ(0px);"></div></div>
        </div>
        <!--Modal Reservation-->
        <div class="modal fade modal-ext" id="modal-reservation" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" style="display: none;" aria-hidden="true">
            <div class="modal-dialog" role="document">
                <!--Content-->
                <div class="modal-content">
                    <!--Header-->
                    <div class="modal-header text-center">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">×</span>
                        </button>
                        <h4 class="modal-title w-100">Reservation Form</h4>
                    </div>
                    <!--Body-->
                    <div class="modal-body">
                        <div class="md-form">
                            <input type="text" id="form22" class="form-control">
                            <label for="form42">Your Full Name</label>
                        </div>
                        <div class="md-form">
                            <input type="text" id="form32" class="form-control">
                            <label for="form34">Your Email</label>
                        </div>
                        <div class="md-form">
                            <input type="text" id="form32" class="form-control">
                            <label for="form34">Your Phone Number</label>
                        </div>
                        <!--Basic textarea-->
                        <div class="md-form">
                            <textarea type="text" id="form7" class="md-textarea"></textarea>
                            <label for="form7">Basic textarea</label>
                        </div>
                        <div class="text-center">
                            <button class="btn btn-lg btn-purple-2 waves-effect">Send Information</button>
                            <p class="text-muted">*Some dummy text goes here.</p>

                            <div class="call">
                                <p>Or you prefer book a table by phone? <span class="cf-phone"><i class="fa fa-phone"></i>+01 234 565 280</span></p>
                            </div>
                        </div>
                    </div>
                    <!--Footer-->
                    <div class="modal-footer">
                        <button type="button" class="btn btn-info waves-effect waves-light" data-dismiss="modal">Close</button>
                    </div>
                </div>
                <!--/Content-->
            </div>
        </div>
        <!--/Modal Reservation-->
        <!--Modal Contact-->
        <div class="modal fade modal-ext" id="modal-contact" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
            <div class="modal-dialog" role="document">
                <!--Content-->
                <div class="modal-content">
                    <!--Header-->
                    <div class="modal-header text-center">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">×</span>
                        </button>
                        <h4 class="modal-title w-100" id="myModalLabel">Write to us</h4>
                    </div>
                    <!--Body-->
                    <div class="modal-body">
                        <p>We like to receive a messages.</p>
                        <br>
                        <div class="md-form">
                            <i class="fa fa-user prefix"></i>
                            <input type="text" id="form22" class="form-control">
                            <label for="form42">Your name</label>
                        </div>
                        <div class="md-form">
                            <i class="fa fa-envelope prefix"></i>
                            <input type="text" id="form32" class="form-control">
                            <label for="form34">Your email</label>
                        </div>
                        <div class="md-form">
                            <i class="fa fa-tag prefix"></i>
                            <input type="text" id="form32" class="form-control">
                            <label for="form34">Subject</label>
                        </div>
                        <div class="md-form">
                            <i class="fa fa-pencil prefix"></i>
                            <textarea type="text" id="form8" class="md-textarea"></textarea>
                            <label for="form8">Icon Prefix</label>
                        </div>
                        <div class="text-center">
                            <button class="btn btn-lg btn-primary waves-effect">Submit</button>
                            <div class="call">
                                <p>Or would you prefer to call? <span class="cf-phone"><i class="fa fa-phone"></i>+01 234 565 280</span></p>
                            </div>
                        </div>
                    </div>
                    <!--Footer-->
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default waves-effect" data-dismiss="modal">Close</button>
                    </div>
                </div>
                <!--/Content-->
            </div>
        </div>
        <!--/Modal Contact-->
    </header>
    <!--/Navigation & Intro-->
    <!--Main content-->
    <main>

        <div class="container body-content">
            @RenderBody()
            <hr />

        </div>
    </main>
    <!--/Main content-->
    <!--Footer-->
    <footer class="page-footer center-on-small-only pt-4">

        <!--Footer Links-->
        <!--/Footer Links-->
        <!--Copyright-->
        <div class="footer-copyright wow fadeIn" data-wow-delay="0.3s" style="visibility: hidden; animation-delay: 0.3s; animation-name: none;">
            <div class="container-fluid">
                © 2016 Copyright: <a href="https://www.mdbootstrap.com/" rel="nofollow"> MDBootstrap.com </a>
            </div>
        </div>
        <!--/Copyright-->
        


    </footer><div class="hiddendiv common"></div>
    <!-- SCRIPTS -->

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/mdb")
    <script>

        //Animation init
        new WOW().init();

        //Modal
        $('#myModal').on('shown.bs.modal', function () {
            $('#myInput').focus()
        })

        // Material Select Initialization
        $(document).ready(function () {
            $('.mdb-select').material_select();
        });

    </script>
    @RenderSection("scripts", required: false)
</body>
</html>
